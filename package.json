{
	"name": "sveltekit-ecommerce",
	"version": "0.1.0",
	"description": "SvelteKit storefront with MedusaJS eCommerce platform",
	"scripts": {
		"dev": "vite dev",
		"build": "shx rm -rf ./build && vite build",
